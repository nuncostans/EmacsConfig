[user]
	name = Volodymyr Yevtushenko
	email = voloyev@vivaldi.net
[core]
	autocrlf = input
	safecrlf = true
[alias]
	co   = checkout
	cim  = commit -m
	st   = status
	br   = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[commit]
	gpgsign = true
